{
  "name": "lastfm-lyric-sheet",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build:mac":
      "electron-packager . 'Lyric Sheet' --platform=darwin --icon assets/icons/icns/icon.icns --out=./build --overwrite",
    "build:linux":
      "electron-packager . 'Lyric Sheet' --platform=linux --out=./build --overwrite",
    "build:windows":
      "electron-packager . 'Lyric Sheet' --platform=win32 --out=./build --overwrite",
    "build": "npm run build:mac && npm run build:linux && npm run build:windows"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": ["Electron", "quick", "start", "tutorial", "demo"],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "electron": "^1.7.5",
    "electron-packager": "^9.0.1"
  },
  "dependencies": {
    "genius-api": "^0.4.1",
    "lastfm": "git+https://git@github.com/shaunchurch/lastfm-node.git",
    "x-ray": "^2.3.2"
  }
}
