{
  "name": "lastfm-lyric-sheet",
  "version": "1.0.0",
  "description": "A minimal Electron application",
  "main": "main.js",
  "scripts": {
    "dev": "concurrently \"BROWSER=none npm start\" \"wait-on http://localhost:3000 && electron .\"",
    "start": "rescripts start",
    "start-electron": "electron .",
    "build:mac": "DEBUG=electron-packager electron-packager . 'Lyric Sheet' --platform=darwin --icon assets/icons/icns/icon.icns --out=./build --overwrite",
    "build:linux": "electron-packager . 'Lyric Sheet' --platform=linux --out=./build --overwrite",
    "build:windows": "electron-packager . 'Lyric Sheet' --platform=win32 --out=./build --overwrite",
    "build": "npm run build:mac && npm run build:linux && npm run build:windows",
    "postinstall": "./node_modules/.bin/electron-rebuild"
  },
  "repository": "https://github.com/electron/electron-quick-start",
  "keywords": [
    "Electron",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "GitHub",
  "license": "CC0-1.0",
  "devDependencies": {
    "concurrently": "^4.1.0",
    "electron": "^5.0.1",
    "electron-packager": "^13.1.1",
    "electron-rebuild": "^1.8.4",
    "wait-on": "^3.2.0"
  },
  "dependencies": {
    "@rescripts/cli": "0.0.10",
    "@rescripts/rescript-env": "0.0.10",
    "@types/jest": "^24.0.12",
    "@types/node": "^12.0.0",
    "@types/react": "^16.8.16",
    "@types/react-dom": "^16.8.4",
    "electron-is-dev": "^1.1.0",
    "genius-api": "^0.4.1",
    "lastfm": "git+https://git@github.com/shaunchurch/lastfm-node.git",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "^3.0.0",
    "sanitize-html": "^1.20.1",
    "typescript": "^3.4.5",
    "x-ray": "^2.3.3"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
